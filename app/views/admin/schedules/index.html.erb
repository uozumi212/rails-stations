<h1>スケジュール一覧</h1>

<% @movies.each do |movie| %>

<% @schedules.each do |schedule| %>
  <% if schedule.movie_id == movie.id %>
  <h2>id：<%= movie.id %>映画タイトル：<%= movie.name %></h2>
  上映時間：<a href="<%= admin_schedule_path(schedule.id) %>"><%= schedule.start_time.strftime('%Y/%m/%d %H:%M') %> ~ <%= schedule.end_time.strftime('%Y/%m/%d %H:%M') %></a>
  <% end %>
<% end %>
<% end %>
